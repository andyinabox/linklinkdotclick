<!DOCTYPE html>
<html class="no-js" lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <title>{{ .Title }}</title>

    <!-- https://github.com/sindresorhus/modern-normalize -->
    <link rel="stylesheet" href="/static/normalize.css" />

    <!-- see original css file at /static/main.css -->
    <link rel="stylesheet" href="/static/main.min.css?{{ .AppVersion }}" />

    <!-- your styles! -->
    <style id="user-styles">
{{ css .UserStyles }}
    </style>
    
    <!-- sourcery -->
    <script defer src="/static/main.min.js?{{ .AppVersion }}"></script>
    
    <!-- analytics -->
    <script data-goatcounter="https://linklinkclick.goatcounter.com/count" async src="//gc.zgo.at/count.js"></script>

    <meta name="description" content="{{ .Description }}" />

    <meta property="og:title" content="{{ .OgTitle }}" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="{{ .Url }}" />
    <meta property="og:image" content="{{ .OgImageUrl }}" />
    <meta property="og:image:alt" content="{{ .OgImageAlt }}" />

    <link rel="icon" href="{{ .FavIconUrl }}" sizes="any" />
    <link rel="apple-touch-icon" href="{{ .AppleTouchIconUrl }}" />
    <link rel="manifest" href="{{ .ManifestUrl }}">
  </head>
